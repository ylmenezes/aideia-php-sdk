{
    "CONFIG": {
        "URL_QA": "https://apiclink.aideia.com/api",
        "URL_PRD": "https://apiclink.aideia.com/api"
    },
    "ENDPOINTS": {
        "auth": {
            "route": "/login",
            "method": "POST"
        },
        "getProdutos": {
            "route": "/produtos",
            "method": "GET"
        },
        "detailProduto": {
            "route": "/produto/:uid",
            "method": "GET"
        },
        "updateProduto": {
            "route": "/produto/:uid",
            "method": "PUT"
        },
        "deleteProduto": {
            "route": "/produto/:uid",
            "method": "DELETE"
        },
        "createProduto": {
            "route": "/produtos",
            "method": "POST"
        },
        "getCategorias": {
            "route": "/categorias",
            "method": "GET"
        },
        "createCategoria": {
            "route": "/categorias",
            "method": "POST"
        },
        "updateCategoria": {
            "route": "/categoria/:uid",
            "method": "PUT"
        },
        "getImagemProduto": {
            "route": "/imagens/produto/:uid",
            "method": "GET"
        },
        "detailImagem": {
            "route": "/imagem/:uid",
            "method": "GET"
        },
        "createImagens": {
            "route": "/imagens/produtos",
            "method": "POST"
        },
        "deleteImagens": {
            "route": "/imagem/:uid",
            "method": "DELETE"
        }
    }
}